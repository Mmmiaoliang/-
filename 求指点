#include<stdio.h>
int main()
{
	int a,b;
	scanf("%d",&a);
	printf("your score:");
	if(90<=a<=100)b=1;
	else if(80<=a<=89)b=2;
	else if(70<=a<=79)b=3;
	else if(60<=a<=69)b=4;
	if(0<=a<=60)b=5;
	switch (b)
	{
	case 1:printf("A\n");break;
	case 2:printf("B\n");break;
	case 3:printf("C\n");break;
	case 4:printf("D\n");break;
	case 5:printf("E\n");break;
	
	}
	return 0;
}
